<h1>ジャンル一覧・追加</h1>
<%= form_with model:@new_genre, url:admin_genres_path, local:true do |f| %>
  <%= f.label :"ジャンル名" %>
  <%= f.text_field :name %>
  <%= f.submit '新規登録' %>
<% end %>

<% @genres.each do |genre| %>
<div>
  <p>
    ジャンル名：
    <%= genre.name %>
    <%= link_to "編集する", edit_admin_genre_path %>
  </p>
</div>
<% end %>