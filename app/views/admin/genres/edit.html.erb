<div class="container">
  <div class="row">
    <div class="col-lg-10">
      <h3>ジャンル編集</h3>

      <table class="table table-borderless">
      <%= form_with model:@edit_genre, url:admin_genre_path, local:true do |f| %>
        <tr>
          <td><%= f.label :"ジャンル名" %></td>
          <td><%= f.text_field :name %></td>
          <td><%= f.submit '変更を保存' %></td>
        </tr>
      <% end %>
      </table>
    </div>
  </div>
</div>